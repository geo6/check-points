{
    "name": "geo6/check-points",
    "type": "project",
    "require": {
        "geo6/zend-acl-postgresql": "^1.0",
        "zendframework/zend-component-installer": "^2.1",
        "zendframework/zend-config": "^3.1",
        "zendframework/zend-config-aggregator": "^1.1",
        "zendframework/zend-db": "^2.9",
        "zendframework/zend-diactoros": "^1.7",
        "zendframework/zend-expressive": "^3.0",
        "zendframework/zend-expressive-authentication": "^0.4.0",
        "zendframework/zend-expressive-authentication-session": "^0.4.0",
        "zendframework/zend-expressive-fastroute": "^3.0",
        "zendframework/zend-expressive-helpers": "^5.0",
        "zendframework/zend-expressive-platesrenderer": "^2.0",
        "zendframework/zend-expressive-session-ext": "^1.0",
        "zendframework/zend-i18n": "^2.7",
        "zendframework/zend-servicemanager": "^3.3"
    },
    "require-dev": {
        "filp/whoops": "^2.1",
        "squizlabs/php_codesniffer": "^3.2",
        "zfcampus/zf-development-mode": "^3.1"
    },
    "authors": [
        {
            "name": "Jonathan BeliÃ«n",
            "email": "jbe@geo6.be"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@development-enable"
        ],
        "development-disable": "zf-development-mode disable",
        "development-enable": "zf-development-mode enable",
        "development-status": "zf-development-mode status",
        "clear-config-cache": "php bin/clear-config-cache.php",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "serve": "php -S 0.0.0.0:8080 -t public/"
    }
}
